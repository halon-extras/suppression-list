{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Suppression schema",
  "additionalProperties": false,
  "properties": {
    "lists": {
      "type": "array",
      "uniqueItems": true,
      "description": "Suppression files",
      "items": {
        "type": "object",
        "required": [
          "id",
          "path"
        ],
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          },
          "path": {
            "type": "string"
          }
        }
      }
    }
  }
}
